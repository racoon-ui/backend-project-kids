  "resolutions": {
    "minimist": "^1.2.5"
  },
  "_moduleAliases": {
    "@components": "dist/components",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares",
    "@config": "dist/config"
  }
