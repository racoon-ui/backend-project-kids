{
  "name": "typescript_nodeapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [
    "typescript",
    "express",
    "restapi",
    "mongodb",
    "redis"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "check-types": "tsc",
    "dev": "npm run check-types && cross-env NODE_ENV=development ts-node ./src/index.ts",
    "prod": "npm run build && pm2 start processes.json --env production --no-daemon",
    "clean": "rimraf ./dist",
    "start": "node dist/index",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:prod": "webpack --mode=production --config webpack.config.ts",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
