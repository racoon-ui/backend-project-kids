    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-acl": "^2.0.8",
    "express-rate-limit": "^5.1.1",
    "express-validation": "^3.0.2",
    "express-winston": "^4.0.3",
    "helmet": "^3.21.3",
    "http-status": "^1.4.2",
    "ioredis": "^4.16.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
